package practice;

import java.util.Iterator;

public class JavaPractice {

	public static void main(String[] args) {

		int arr[] = { 0, 01, 1, 2, 0, 3, 4, 5, 60, };
		int count = 0;

		for (int i = 0; i < arr.length; i++) {

			if (arr[i] != 0) {

				arr[count++] = arr[i];

			}

		}
		
		while(count<arr.length) {
			
			arr[count++]=0;
			
		}
		
		for(int a : arr) {
			
			System.out.print(a+" ");
			
		}
		
		
	}
}