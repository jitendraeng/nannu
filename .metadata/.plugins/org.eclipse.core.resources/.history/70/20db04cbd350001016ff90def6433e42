package PracticeJava;

public class StringExamples {

	public static void main(String[] args) {
		String s = "radar";
		char[] c = s.toCharArray();
		char a=' ';

		for (int i = 0; i < c.length-1; i++) {
			int count = 0;
			for (int j = 1; j < c.length; j++) {

				if (c[i] == c[j]) {
                    
					count++;

				}
                 a=(char) (a+c[j]);
			}
      System.out.println(a+" "+count);
		}

	}

}
