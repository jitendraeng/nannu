public class StringExp {

	public static void main(String[] args) {

		int y=2024;
		
		if(y%4==0 || y%100!=0 && y%400==0) {
			
			System.out.println("its a leep year");
			
		}else {
			System.out.println("its not a leep year");
			
		}
		
	}

}