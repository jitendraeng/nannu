package practice;

public class StringExp {

	public static void main(String[] args) {

		String a = "successful";
		int count = 0;
		char[] c = a.toCharArray();
		for (int i = 0; i < c.length; i++) {
			
			boolean dup = false;
			for (int j = i + 1; j < c.length; j++) {

				if (c[i] == c[j]) {
					dup = true;
					count++;

				}

			}
			if(dup) {
			System.out.println(c[i]+" "+count);
			
		}

	}
}}