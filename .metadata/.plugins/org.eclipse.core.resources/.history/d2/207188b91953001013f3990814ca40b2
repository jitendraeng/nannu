package practice;

import java.util.Arrays;

public class JavaPractice {

	public static void main(String[] args) {

		int arr[] = { 2, 0, 3, 1, 5, 6, 4, 7, 8, 9 };

		int a = 0, b = 0, sum = 0;
		int x = 8;
		sum = a + b;

		for (int i = 0; i < arr.length - 1; i++) {

			for (int j = i+1; j < arr.length; j++) {

				if (arr[i] + arr[j] == x) {
					
					a=arr[i];
					b=arr[j];
				System.out.println(a+"values"+b);

				}

			}
			
			

		}

	}

}
