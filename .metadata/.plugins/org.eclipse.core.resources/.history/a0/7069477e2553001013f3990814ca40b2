package practice;

import java.util.Iterator;

public class JavaPractice {

	public static void main(String[] args) {

		int arr[] = { 0, 0, 1, 2, 0, 3, 4, 5, 60, };
		int count = 0;

		for (int i = 0; i < arr.length; i++) {

			if (arr[i] == 0) {

				count++;

			}
			int arr1[] = new int[arr.length];
			int size = 0;

			if (arr[i] != 0) {

				arr1[size++] = arr[i];

			}
			
			for(int s: arr1) {
				
				System.out.print(s+" ");
				
			}

		}
		
		
		
		
	}
}