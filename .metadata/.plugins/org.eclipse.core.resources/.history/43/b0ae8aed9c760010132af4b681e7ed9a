package practice;

public class StringExp {

	public static void main(String[] args) {

		String a = "successful";
		int count = 0;
		char[] c = a.toCharArray();
		for (int i = 0; i < c.length; i++) {

			boolean dup = false;
			for (int j = 0 ; j < c.length; j++) {

				if (c[i] == c[j]) {
					dup = true;
					break;
				}

			}
			if (dup) {
				System.out.println(c[i] );

			}
			
		}
	}
}