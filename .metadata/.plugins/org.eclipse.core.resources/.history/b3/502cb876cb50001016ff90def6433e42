package PracticeJava;

public class StringExamples {

	public static void main(String[] args) {
		String s = "radar";
		char[] c = s.toCharArray();
		char a=' ';

		for (int i = 0; i < c.length; i++) {
			int count = 0;
			for (int j = 0; j < i; j++) {

				if (c[i] == c[j]) {
                    a=c[i];
					count++;

				}

			}
      System.out.println(a+" "+count);
		}

	}

}
